<rdf:RDF
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:oai="http://www.mediaspaces.info/vocab/oai-pmh.rdf#">
  <owl:Ontology rdf:about="">
    <foaf:page>
      <rdf:Description rdf:about="http://www.mediaspaces.info/index.php?dir=tools&amp;page=oai2lod">
        <rdfs:label xml:lang="en">OAI2LOD Server homepage and documentation</rdfs:label>
      </rdf:Description>
    </foaf:page>
    <rdfs:comment xml:lang="en">This vocabulary defines RDF terms for the OAI-PMH protocol.</rdfs:comment>
    <rdfs:label xml:lang="en">OAI-PMH Vocabulary.</rdfs:label>
  </owl:Ontology>
  <rdfs:Class rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Set">
    <rdfs:isDefinedBy rdf:resource="http://www.openarchives.org/OAI/openarchivesprotocol.html#Set"/>
    <rdfs:comment>A set is an optional construct for grouping items for the purpose of selective harvesting</rdfs:comment>
    <rdfs:label xml:lang="en">Set</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Item">
    <rdfs:isDefinedBy rdf:resource="http://www.openarchives.org/OAI/openarchivesprotocol.html#Item"/>
    <rdfs:comment xml:lang="en">A constituent of a repository from which metadata about a resource can be disseminated.</rdfs:comment>
    <rdfs:label xml:lang="en">Item</rdfs:label>
  </rdfs:Class>
  <rdf:Property rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#setSpec">
    <rdfs:isDefinedBy rdf:resource="http://www.openarchives.org/OAI/openarchivesprotocol.html#Set"/>
    <rdfs:domain rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Set"/>
    <rdfs:comment xml:lang="en">A unique identifier for the set within the repository.</rdfs:comment>
    <rdfs:label xml:lang="en">setSpec</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#origin">
    <rdfs:domain rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Item"/>
    <rdfs:domain rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Set"/>
    <rdfs:subproperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso"/>
    <rdfs:comment xml:lang="en">Links to the origin of an OAI-PMH Item or Set harvested by the OAI2LOD Server.</rdfs:comment>
    <rdfs:label xml:lang="en">origin</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#set">
    <rdfs:range rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Set"/>
    <rdfs:domain rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Item"/>
    <rdfs:comment xml:lang="en">The set an item belongs to.</rdfs:comment>
    <rdfs:label xml:lang="en">set</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#setName">
    <rdfs:isDefinedBy rdf:resource="http://www.openarchives.org/OAI/openarchivesprotocol.html#Set"/>
    <rdfs:domain rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Set"/>
    <rdfs:comment xml:lang="en">A short human-readable string naming the set.</rdfs:comment>
    <rdfs:label xml:lang="en">setName</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.mediaspaces.info/vocab/oai-pmh.rdf#setDescription">
    <rdfs:isDefinedBy rdf:resource="http://www.openarchives.org/OAI/openarchivesprotocol.html#Set"/>
    <rdfs:domain rdf:resource="http://www.mediaspaces.info/vocab/oai-pmh.rdf#Set"/>
    <rdfs:comment xml:lang="en">Holds community-specific descriptions about the set.</rdfs:comment>
    <rdfs:label xml:lang="en">setSpec</rdfs:label>
  </rdf:Property>
</rdf:RDF>
